      </ImageView>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="80.0" prefWidth="170.0" spacing="10.0">
         <children>
            <Label id="name" fx:id="foodName" text="음식이름" />
            <Label id="description" prefHeight="44.0" prefWidth="155.0" text="설명내용" wrapText="true" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
