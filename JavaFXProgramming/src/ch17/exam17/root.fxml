<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="150.0" prefWidth="420.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch17.exam17.RootController">
    <children>
        <CheckBox fx:id="cb1" layoutX="24.0" layoutY="31.0" mnemonicParsing="false" text="안경" />
        <CheckBox fx:id="cb2" layoutX="24.0" layoutY="67.0" mnemonicParsing="false" text="모자" />
        <ImageView fx:id="checkImageView" fitHeight="87.0" fitWidth="87.0" layoutX="86.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@images/geek.gif" />
            </image>
        </ImageView>
        <Button fx:id="btnClose" layoutX="183.0" layoutY="106.0" mnemonicParsing="false" text="닫기">
            <graphic>
                <ImageView>
                    <image>
                        <Image url="@images/exit.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <RadioButton fx:id="rb1" userData="BubbleChart" layoutX="195.0" layoutY="23.0" mnemonicParsing="false" selected="true" text="BubbleChart">
            <toggleGroup>
                <ToggleGroup fx:id="rbGroup" />
            </toggleGroup>
        </RadioButton>
        <RadioButton fx:id="rb2" userData="BarChart" layoutX="195.0" layoutY="47.0" mnemonicParsing="false" text="BarChart" toggleGroup="$rbGroup" />
        <RadioButton fx:id="rb3" userData="AreaChart" layoutX="196.0" layoutY="71.0" mnemonicParsing="false" text="AreaChart" toggleGroup="$rbGroup" />
        <ImageView fx:id="radioImageView" fitHeight="86.0" fitWidth="107.0" layoutX="313.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@images/BubbleChart.png" />
            </image>
        </ImageView>
    </children>
</AnchorPane>
